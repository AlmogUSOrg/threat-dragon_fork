{
  "name": "bower.json",
  "version": "0.0.0",
  "authors": [
    "mike-goodwin <mike.goodwin@sage.com>"
  ],
  "description": "OWASP Threat Dragon",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "public/scripts/",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.4.3",
    "angular-route": "~1.4.3",
    "angular-ui-bootstrap-bower": "~0.13.0",
    "nools": "*",
    "jquery": "~2.0.3",
    "lodash": "~3.10.1",
    "backbone": "~1.2.1",
    "toastr": "~2.1.1",
    "bootstrap": "~3.3.5",
    "joint": "git://github.com/clientIO/joint#~0.9.3",
    "font-awesome": "~4.3.0",
    "angular-xeditable": "~0.1.9"
  },
  "devDependencies": {
    "angular-mocks": "~1.4.7",
    "jasmine-jquery": "~2.1.1",
    "Blob": "*"
  },
  "exportsOverride": {
    "joint": {
      ".": [
        "dist/joint.js",
        "dist/joint.css"
      ]
    },
    "bootstrap": {
      ".": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css"
      ],
      "../fonts": "fonts/*"
    },
    "nools": {
      ".": "nools.js"
    },
    "Blob": {
      ".": "Blob.js"
    },
    "font-awesome": {
      ".": "css/font-awesome.css",
      "../fonts": "fonts/*"
    }
  },
  "overrides": {
    "joint": {
      "dependencies": {},
      "main": [
        "dist/joint.js",
        "dist/joint.css"
      ]
    },
    "backbone": {
      "dependencies": {}
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css"
      ]
    }
  },
  "resolutions": {
    "jquery": "~2.0.3",
    "angular": ">=1.3.0"
  }
}
