{
  "name": "threat-dragon",
  "productName": "Threat Dragon",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:desktop": "vite electron:build",
    "clean": "rimraf coverage/ cypress/ dist/ dist-desktop/ node_modules/ tests/e2e/videos/ tests/e2e/reports/ tests/e2e/screenshots/ log/ results/",
    "cypress:open": "cypress open",
    "dev": "vite --port 8080",
    "lint": "eslint --ext .js,.vue --fix src",
    "preinstall": "npx only-allow pnpm",
    "pretest": "npm run lint",
    "serve": "vite preview",
    "start": "echo 'Please run the start command from the root of this repository' && exit 1",
    "start:desktop": "electron .",
    "start:serve": "pm2 --name td.vue start 'vite preview'",
    "stop:serve": "pm2 delete td.vue",
    "test": "vitest",
    "test:desktop": "vite test:unit --testMatch '**/unit/desktop/**/*.spec.js' --collectCoverageFrom='src/desktop/**/*.js'",
    "test:e2e": "vite test:e2e -C e2e.local.config.js",
    "test:e2e:desktop": "wdio --verbose run ./wdio.config.js",
    "test:e2e:local": "vite test:e2e -C e2e.local.config.js --url http://localhost:8080/",
    "test:e2e-ci": "vite test:e2e  -C e2e.ci.config.js --browser firefox --headless --url http://localhost:3000/",
    "test:e2e-ci-smokes": "vite test:e2e -C e2e.smokes.ci.config.js --browser firefox --headless --url http://localhost:3000/",
    "test:e2e-smokes": "browserstack-cypress run --cf browserstack.smokes.json --sync",
    "test:e2e-smokes:local": "vite test:e2e -C e2e.smokes.local.config.js --url http://localhost:8080/",
    "test:e2e-nightly": "browserstack-cypress run --cf browserstack.nightly.json --sync",
    "test:unit": "vitest run --coverage",
    "test:vue": "vite test:e2e -C e2e.local.config.js --headless --browser chrome"
  },
  "description": "OWASP Threat Dragon - a free, open source threat modeling tool",
  "author": {
    "name": "OWASP Threat Dragon",
    "email": "jon.gadsden@owasp.org",
    "url": "https://owasp.org/www-project-threat-dragon/"
  },
  "license": "Apache-2.0",
  "homepage": "https://owasp.org/www-project-threat-dragon/",
  "buildState": "-draft",
  "repository": {
    "type": "git",
    "url": "git://github.com/OWASP/threat-dragon.git"
  },
  "bugs": {
    "email": "jon.gadsden@owasp.org",
    "url": "https://github.com/OWASP/threat-dragon/issues"
  },
  "dependencies": {
    "@antv/x6": "^1.34.12",
    "@antv/x6-vue-shape": "^1.5.4",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.7",
    "@vue/compat": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "ajv": "^8.12.0",
    "axios": "^1.3.6",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "dotenv": "^8.6.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "is-electron": "^2.2.2",
    "jquery": "^3.6.4",
    "pinia": "^2.0.35",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.16",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.3.2",
    "@wdio/cli": "^8.6.9",
    "@wdio/local-runner": "^8.6.9",
    "@wdio/mocha-framework": "^8.6.8",
    "ansi-regex": "5.0.1",
    "browserstack-cypress-cli": "^1.20.0",
    "c8": "^7.13.0",
    "chromedriver": "^104.0.0",
    "cypress": "^12.9.0",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "electron-builder-notarize": "^1.5.1",
    "electron-chromedriver": "^18.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest": "^0.1.4",
    "eslint-plugin-vitest-globals": "^1.3.1",
