    "npm-force-resolutions": "^0.0.10",
    "pm2": "^5.3.0",
    "rimraf": "^4.4.1",
    "sass": "^1.62.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.1",
    "vitest": "^0.30.1",
    "wdio-electron-service": "^4.0.2",
    "webdriverio": "^8.6.9"
  },

  "overrides": {
    "node-ipc": ">=10.1.7",
    "debug@<3.1.0": ">=4.3.4",
    "follow-redirects": ">=1.14.8",
    "glob-parent@<5.1.2": ">=5.1.2",
    "lodash@<4.17.19": ">=4.17.19",
    "minimist@>=1.0.0 <1.2.3": ">=1.2.3",
    "node-fetch": ">=2.6.7",
    "node-notifier@<8.0.1": ">=8.0.1",
    "nth-check@<2.0.1": ">=2.0.1",
    "tar@>=6.0.0 <6.1.9": ">=6.1.9",
    "webpack@<5.0.0": ">=5.77.0",
    "yargs-parser@>=6.0.0 <13.1.2": ">=13.1.2"
  },
  "main": "background.js",
  "resolutions": {
    "ansi-regex": "^6.0.1"
  }
}
