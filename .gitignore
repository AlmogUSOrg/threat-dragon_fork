# uses allow-list, so ignore everything
*

# allow github, workflows and templates
!.github
!.github/**/*
!.gitignore
!.gitattributes
!.lycheeignore
!.trivyignore

# docker files
!.dockerignore
!Dockerfile

# allow build system and artwork
!*.md
!LICENSE.txt
!cupcake/icons/*.ico
!cupcake/png/*.png
!cupcake/png/*.gif
!example.env
!package.json
!package-lock.json

# sample threat models
!ThreatDragonModels
!ThreatDragonModels/**/*.json

# documentation site
!docs
!docs/Gemfile
!docs/Gemfile.lock
!docs/*.md
!docs/*.yml
!docs/*.html
!docs/_layouts/
!docs/_layouts/*.html

# Express server
!td.server
!td.server/**/*.js
!td.server/.babelrc
!td.server/README.md
!td.server/package.json
!td.server/package-lock.json

# Vue front end
!td.vue
!td.vue/**/*.vue
!td.vue/**/*.js
!td.vue/**/*.scss
!td.vue/tests/e2e/fixtures/*.json
!td.vue/src/icons/*
!td.vue/src/assets/*
!td.vue/public/*
!td.vue/package.json
!td.vue/package-lock.json
!td.vue/.browserslistrc
!td.vue/.editorconfig
!td.vue/README.md
!td.vue/browserstack.nightly.json
!td.vue/browserstack.smokes.json

# utility scripts
!utils
!utils/**/*
